(define (domain pr2-tamp)
	(:requirements :strips :equality)
	(:predicates
		(controllable )
		(stackable ?o ?r)
		(grasp ?o ?g)
		(kin ?o ?p ?g ?q ?t)
		(basemotion ?q1 ?t ?q2)
		(armmotion ?q1 ?t ?q2)
		(supported ?o ?p ?r)
		(atpose ?o ?p)
		(atgrasp ?o ?g)
		(handempty )
		(atbconf ?q)
		(ataconf ?q)
		(on ?o ?r)
		(blue ?o)
		(blue_region ?o)
		(red ?o)
		(red_region ?o)
		(holding ?o)
		(ontarget ?o)
		(= ?x ?y)
	)
	(:functions
	)
	(:action pick
		:parameters (?o ?p ?g ?q ?t)
		:precondition (and (kin ?o ?p ?g ?q ?t) (atpose ?o ?p) (grasp ?o ?g) (handempty ))
		:effect (and (atgrasp ?o ?g) (not (atpose ?o ?p)) (not (handempty )))
	)
	(:action place
		:parameters (?o ?p ?g ?q ?t)
		:precondition (and (kin ?o ?p ?g ?q ?t) (atgrasp ?o ?g) (not (handempty )))
		:effect (and (atpose ?o ?p) (not (atgrasp ?o ?g)) (handempty ))
	)
	(:derived (on ?o ?r)
		(exists (?p) (and (supported ?o ?p ?r) (atpose ?o ?p)))
	)
	(:derived (ontarget ?o)
		(exists (?r) (or (and (red ?o) (on ?o ?r) (red_region ?r)) (and (blue ?o) (on ?o ?r) (blue_region ?r))))
	)
	(:derived (holding ?o)
		(exists (?g) (and (grasp ?o ?g) (atgrasp ?o ?g)))
	)
)
